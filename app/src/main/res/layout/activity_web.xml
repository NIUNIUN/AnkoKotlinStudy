<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <android.support.v7.widget.Toolbar
        android:id="@+id/tl_web"
        android:layout_width="match_parent"
        android:layout_height="@dimen/comm_40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/comm_green_38b487"
        android:gravity="center"
        app:titleTextColor="@color/comm_white"
        />
    <WebView
        android:id="@+id/wv_web"
        android:layout_width="match_parent"
        android:layout_height="@dimen/comm_0dp"
        app:layout_constraintTop_toBottomOf="@+id/tl_web"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />
</android.support.constraint.ConstraintLayout>